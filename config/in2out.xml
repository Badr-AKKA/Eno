<?xml version="1.0" encoding="UTF-8"?>
<!-- This file contains common properties used by the in2out transformation  -->
<Properties>
    <!-- The name of the survey, conditionning the name of the input file processed. -->
    <SurveyName>simpsons</SurveyName>
    
    <!-- The in-format to be read : ddi / xml-pogues -->
    <InFormat>ddi</InFormat>
    
    <!-- The out-format to be written : fr / pdf / odt / ddi / xml-pogues -->
    <OutFormat>fr</OutFormat>
    
    <!-- debug mode : true / false -->
    <Debug>false</Debug>
</Properties>